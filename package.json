{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsx --watch --env-file .env src/index.ts",
    "dev:docker": "docker compose --env-file .env up",
    "build": "tsc && cp -r src/express/swagger dist/express && cp -r src/databases/postgre/migrations dist/databases/postgre/migrations",
    "start": "npm run migration:run:prod && node dist/index.js",
    "migrate": "node-pg-migrate",
    "migration:create": "node create-migration.mjs",
    "migration:run": "node-pg-migrate -m src/databases/postgre/migrations up",
    "migration:run:prod": "echo 'Running migrations...' && node-pg-migrate -m dist/databases/postgre/migrations up",
    "start:local": "npm run migration:run:prod && node dist/index.js",
    "migration:down": "node-pg-migrate -m src/databases/postgre/migrations down"
  },
  "keywords": [],
