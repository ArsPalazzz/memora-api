    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "http-errors": "^2.0.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "node-pg-migrate": "^8.0.3",
    "pg": "^8.16.3",
    "redis": "^4.7.0",
    "swagger-ui-express": "^5.0.1",
    "tsx": "^4.20.6",
    "unique-names-generator": "^4.7.1",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/pg": "^8.15.6",
    "@types/swagger-ui-express": "^4.1.8",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  }
